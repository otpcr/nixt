#!/usr/bin/env python3
# This file is placed in the Public Domain.


"skip .git"


import os
import sys


def popen(txt):
    "run popen command."
    for line in os.popen(txt).readlines():
        print(line.rstrip())
        sys.stdout.flush()


os.popen(r"tar zcvf nixt.tar.gz --exclude='nixt/\.git/*' nixt")
